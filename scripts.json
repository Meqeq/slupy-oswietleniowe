{
  "$schema": "https://deno.land/x/denon@2.5.0/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run app.ts",
	"desc": "run my app.ts file"
    },
    "dev": {
	"cmd": "deno run --allow-net --allow-read --allow-write --allow-env --allow-run -c tsconfig.json app.ts"
    },
    "test": {
	"cmd": "deno test --allow-net --allow-read --allow-write --allow-env -c tsconfig.json"
    }

  }
}
